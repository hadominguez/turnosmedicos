@model Turnos_Medicos.Models.PacienteObraSocial

@{
    ViewBag.Title = "Eliminar Paciente - Obra Social";
}

<h2>@ViewBag.Title</h2>

<h3>¿Esta seguro/a que desea eliminar el Paciente - Obra Social?</h3>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.LabelFor(model => model.NumeroAfiliado, "Número Afiliado", htmlAttributes: new { @class = "control-label" })
        </dt>

        <dd>
       
            @Html.DisplayFor(model => model.NumeroAfiliado)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ObraSocial.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ObraSocial.Nombre)
        </dd>


    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" />
        </div>
    }
</div>
